{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.vs": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.fbx": true,
        "**/*.lxo": true,
        "**/*.ma": true,
        "**/*.obj": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "build/": true,
        "Build/": true,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "Logs/": true,
        "logs/": true,
        "ProjectSettings/": true,
        "UserSettings/": true,
        "temp/": true,
        "Temp/": true,
        "**/*.slnx": true,
        "**/Canonical.cs": true
    },
    "files.associations": {
        "*.asset": "yaml",
        "*.meta": "yaml",
        "*.prefab": "yaml",
        "*.unity": "yaml"
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.sln": "*.csproj"
    },
    "omnisharp.useGlobalMono": "never",
    "omnisharp.path": "latest",
    "omnisharp.dotnetPath": "C:\\Program Files\\dotnet\\dotnet.exe",
    "dotnet.defaultSolution": "My project.slnx",
    "csharp.suppressDotnetRestoreNotification": true,
    "csharp.suppressMSBuildDiagnosticWarning": true,
    "csharp.format.enable": true,
    "csharp.suggestNamespaces": true,
    "dotnet.codeLens.enableReferencesCodeLens": true,
    "dotnet.codeLens.enableTestsCodeLens": false
}